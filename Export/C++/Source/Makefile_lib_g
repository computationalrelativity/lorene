SHELL=/bin/sh

include $(HOME_LORENE)/local_settings		  # defines which compiler,...

include List_obj 		# defines OBJ as the list of object files

.SUFFIXES : .o .C

LIB	  = $(HOME_LORENE)/Lib
INCEXP	  = $(INC) -I$(HOME_LORENE)/Export/C++/Include

$(LIB)/date_mark : $(OBJ)
	ar -rv $(LIB)/liblorene_export_g.a  $?
	$(RANLIB) $(LIB)/liblorene_export_g.a
	
include Dependencies

.C.o:
	$(CXX)  -c $(CXXFLAGS_G) $(INCEXP) $<
